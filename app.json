{
  "name": "SHARK MD",
  "description": "Javascript WhatsApp bot made by  Brian",
  "logo": "https://files.catbox.moe/k4h5mm.png",
  "keywords": ["SHARK-MD"],
  "success_url": "/",

    "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "Session id. Must start with prefix 'POPKID;;;' followed by the token.",
      "required": false,
      "value": ""
    },
      "BOT_NAME": {
        "description": "write here your bot name.",
        "required": true,
        "value": "SHARK MD"      
    },  
    "MODE": {
      "description": "Set private/public/inbox/groups",
      "required": false,
      "value": "public"
     },
   
    "PREFIX": {
      "description": "Your Bot Prefix (default: .) You can put your own custom prefix",
      "required": true,
      "value": "."
    },
    
    "OWNER_NAME": {
      "description": "Put Owner Name Here",
      "required": true,
      "value": "STARBOY"
    },
    "OWNER_NUMBER": {
      "description": "Put only one Owner Number Here",
      "required": true,
      "value": "255627417402"
    },
    
    "PRESENCE": {
      "description": "Put online/typing/recording/unavailable",
      "required": true,
      "value": "online"
    }

    ,
    "SESSION_URL": {
      "description": "Optional URL to session creds (mega.nz or direct HTTPS). Preferred for Heroku.",
      "required": false,
      "value": ""
    }

},

     "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
        }
     ],
  "stack": "heroku-24"
}
  
