{
  "name": "SHARK MD",
  "description": "Javascript WhatsApp bot made by  Brian",
  "logo": "https://files.catbox.moe/k4h5mm.png",
  "keywords": ["SHARK-MD"],
  "success_url": "/",

    "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "Set your session id exactly as: POPKID;;;",
      "required": false,
      "value": "POPKID;;;"
    },
      "BOT_NAME": {
        "description": "write here your bot name.",
        "required": true,
        "value": "SHARK MD"      
    },  
    "MODE": {
      "description": "Set private/public/inbox/groups",
      "required": false,
      "value": "public"
     },
   
    "PREFIX": {
      "description": "Your Bot Prefix (default: .) You can put your own custom prefix",
      "required": true,
      "value": "."
    },
    
    "OWNER_NAME": {
      "description": "Put Owner Name Here",
      "required": true,
      "value": "STARBOY"
    },
    "OWNER_NUMBER": {
      "description": "Put only one Owner Number Here",
      "required": true,
      "value": "255627417402"
    },
    
    "PRESENCE": {
      "description": "Put online/typing/recording/unavailable",
      "required": true,
      "value": "online"
    }

    ,
    "SESSION_URL": {
      "description": "Optional direct URL to session file (supports mega.nz links). When set on Heroku, the bot will auto-download it.",
      "required": false,
      "value": ""
    }

},

     "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
        }
     ],
  "stack": "heroku-24"
}
  
