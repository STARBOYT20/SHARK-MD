{
  "name": "SHARK-MD",
  "description": "Javascript WhatsApp bot made by Brian",
  "repository": "https://github.com/STARBOYT20/SHARK-MD",
  "logo": "https://n.uguu.se/enxBsCxO.jpg",
  "keywords": ["SHARK-MD"],
  "success_url": "/",
  "env": {
    "SESSION_ID": {
      "description": "Provide SESSION_ID starting with 'POPKID;;;' or set SESSION_URL",
      "required": false,
      "value": ""
    },
      "BOT_NAME": {
        "description": "write here your bot name.",
        "required": true,
        "value": "SHARK-MD"      
    },  
    "MODE": {
      "description": "Set private/public/inbox/groups",
      "required": false,
      "value": "public"
     },
   
    "PREFIX": {
      "description": "Your Bot Prefix (default: .) You can put your own custom prefix",
      "required": true,
      "value": "."
    },
    
    "OWNER_NAME": {
      "description": "Put Owner Name Here",
      "required": true,
      "value": "SHARK"
    },
    "OWNER_NUMBER": {
      "description": "Put only one Owner Number Here",
      "required": true,
      "value": "255612491554"
    },
    
    "PRESENCE": {
      "description": "Put online/typing/recording/unavailable",
      "required": true,
      "value": "online"
    }

},

  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    }
  ],
  "stack": "heroku-24"
}
  
